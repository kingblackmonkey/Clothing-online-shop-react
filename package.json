{
  "name": "andy-clothing",
  "version": "1.0.0",
  "engines": {
    "node": "v10.16.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "express": "^4.17.3",
    "handlebars": "^4.5.3",
    "hogan.js": "^3.0.2",
    "nodemailer": "^6.3.1",
    "nodemon": "^1.19.4",
    "stripe": "6.28.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
